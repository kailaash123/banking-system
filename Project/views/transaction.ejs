		<!-- Page Wrapper -->
		<div class="page-wrapper">
			<div class="content container-fluid">

				<!-- Page Header -->
				<div class="page-header">
					<div class="row">
						<div class="col">
							<h3 class="page-title">Transaction</h3>
							<ul class="breadcrumb">
							</ul>
						</div>
					</div>
				</div>
				<!-- /Page Header -->

				<div class="row">
					<div class="col-lg-12">
						<div class="card">
							<div class="card-header">
								<h4 class="card-title">Enter transaction amount</h4>
							</div>
							<div class="card-body">
								<form action="/addteach" onsubmit="return false" method="POST">
									<div class="form-group row">
										<label class="col-form-label col-md-2">Current account</label>
										<div class="col-md-10">
											<h4><%= accn %></h4>
										</div>
									</div>
									<div class="form-group row">
										<label class="col-form-label col-md-2">To account</label>
										<div class="col-md-10">
											<input name="accn" id="accn" type="text" class="form-control">
										</div>
									</div>
									<div class="form-group row">
										<label class="col-form-label col-md-2">Amount</label>
										<div class="col-md-10">
											<input name="amt" id="amt" type="number" class="form-control">
										</div>
									</div>
									<div class="input-group-append">
										<button onclick="clickfn('<%= accn %>')" class="btn btn-primary">Submit</button>
									</div>
								</form>
							</div>
						</div>

					</div>
				</div>

			</div>
		</div>
